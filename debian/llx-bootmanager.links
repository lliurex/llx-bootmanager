# IPXE MODULE (OVERWRITE PXE BIOS)
/usr/lib/ipxe/undionly.kpxe /var/lib/tftpboot/llxbootmanager/undionly.kpxe

# UEFI MODULE
/usr/lib/ipxe/ipxe.efi  /var/lib/tftpboot/llxbootmanager/ipxe.efi

# PXE MODULE
/usr/lib/PXELINUX/pxelinux.0 /var/lib/tftpboot/llxbootmanager/pxelinux.0

# HTTP SERVED
/var/lib/tftpboot/llxbootmanager /usr/share/llxbootmanager/www-boot/pxe

# SYSLINUX MAIN MODULES
/usr/lib/syslinux/modules/bios/ldlinux.c32 /usr/share/llxbootmanager/www-boot/ldlinux.c32
/usr/lib/syslinux/modules/bios/vesamenu.c32 /usr/share/llxbootmanager/www-boot/vesamenu.c32
/usr/lib/syslinux/modules/bios/libcom32.c32 /usr/share/llxbootmanager/www-boot/libcom32.c32
/usr/lib/syslinux/modules/bios/libcom32.c32 /var/lib/tftpboot/ltsp/netinstall/libcom32.c32
/usr/lib/syslinux/modules/bios/libutil.c32 /usr/share/llxbootmanager/www-boot/libutil.c32
/usr/lib/syslinux/modules/bios/libutil.c32 /var/lib/tftpboot/ltsp/netinstall/libutil.c32

# HTTP SERVED
/usr/share/llxbootmanager/www-boot /var/www/ipxeboot

# CHAIN TO BOOT FROM DISK
/usr/lib/syslinux/modules/bios/chain.c32 /var/lib/tftpboot/llxbootmanager/chain.c32
/usr/lib/syslinux/modules/bios/chain.c32 /usr/share/llxbootmanager/www-boot/chain.c32



# TFTP BOOT (NOT IPXE) (NOT HTTP)
/var/lib/tftpboot/ltsp/netinstall/ubuntu-installer/amd64/pxelinux-stats.cfg /var/lib/tftpboot/llxbootmanager/pxelinux.cfg
/usr/lib/syslinux/modules/bios/ldlinux.c32 /var/lib/tftpboot/llxbootmanager/ldlinux.c32
/usr/lib/syslinux/modules/bios/libutil.c32 /var/lib/tftpboot/llxbootmanager/libutil.c32
/usr/lib/syslinux/modules/bios/libcom32.c32 /var/lib/tftpboot/llxbootmanager/libcom32.c32
/var/lib/tftpboot/ltsp/netinstall/ubuntu-installer /var/lib/tftpboot/llxbootmanager/ubuntu-installer



# HDT MODULE
/usr/lib/syslinux/modules/bios/hdt.c32 /var/lib/tftpboot/llxbootmanager/hdt.c32
/usr/lib/syslinux/modules/bios/libmenu.c32 /var/lib/tftpboot/llxbootmanager/libmenu.c32
/usr/lib/syslinux/modules/bios/libgpl.c32 /var/lib/tftpboot/llxbootmanager/libgpl.c32
# HDT MODULE SERVED FROM HTTPD IPXE
/usr/lib/syslinux/modules/bios/hdt.c32 /var/lib/tftpboot/ltsp/netinstall/hdt.c32 
/usr/lib/syslinux/modules/bios/libmenu.c32 /var/lib/tftpboot/ltsp/netinstall/libmenu.c32
/usr/lib/syslinux/modules/bios/libgpl.c32 /var/lib/tftpboot/ltsp/netinstall/libgpl.c32

# MEMTEST86
/boot/memtest86+.bin /var/lib/tftpboot/llxbootmanager/memtest86+.bin
# HDT MODULE SERVED FROM HTTPD IPXE
/boot/memtest86+.bin /var/lib/tftpboot/ltsp/netinstall/memtest86+.bin
 
